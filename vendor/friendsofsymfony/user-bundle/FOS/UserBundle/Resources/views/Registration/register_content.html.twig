{% trans_default_domain 'FOSUserBundle' %}
<div class="login-wrap">
  <h2>Register</h2>
  <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form">
    {{ form_widget(form) }}
    <div>
      <button type="submit" value="{{ 'registration.submit'|trans }}">Submit</button>
    </div>
  </form>
  <ol class="breadcrumb">
    <li>
      <i class="fa fa-sign-in"></i> <a href="{{ path('fos_user_security_login') }}">Login</a>
    </li>
  </ol>
</div>
<div class="login-wrap2">
  <div class="intro">
    {% block info %}
      {% include 'MediashareAppBundle::_infoServer.html.twig' %}
    {% endblock %}
  </div>
</div>